  "version": "1.0.0",
  "description": "Authentication Backend API with Express, Sequelize, and PostgreSQL",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx watch src/index.ts",
    "migrate": "tsx src/scripts/migrate.ts",
    "type-check": "tsc --noEmit"
